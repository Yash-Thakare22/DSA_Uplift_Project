6)
class MinStack {
private:
    stack<int> stk;
    stack<int> min;
public:
    /** initialize your data structure here. */
    MinStack() {

    }

    void push(int x) {
        stk.push(x);
        if(min.empty() || min.top()>=x) 
            min.push(x);
    }

    void pop() {
        if(stk.top()==min.top()) {
            min.pop();
            stk.pop();
        }
        else 
            stk.pop();
    }

    int top() {
        if(!stk.empty())
            return stk.top();
        else
            return NULL;
    }

    int getMin() {
        if(!stk.empty())
            return min.top();
        else
            return NULL;
    }
};
