// https://leetcode.com/problems/unique-paths/

class Solution {
public:
    int uniquePaths(int m, int n) {
        vector<int> gridval(n,1);
        for(int i=1;i<m;i++){
            for(int j=1;j<n;j++){
                gridval[j]+=gridval[j-1];
            }
        }
        return gridval[n-1];
    }
};
