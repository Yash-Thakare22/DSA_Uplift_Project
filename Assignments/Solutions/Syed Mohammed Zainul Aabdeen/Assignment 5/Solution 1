#include<iostream>
using namespace std;
int main() {
    int n;
    cout<<"enter size of binary array: ";
    cin>>n;
    cout<<"enter the elements(0's & 1's): ";
    int a[n];
    for(int i=0;i<n;i++)
        cin>>a[i];
    for(int i=0;i<n-1;i++){
        for(int j=i+1;j<n;j++){
            if(a[i]>a[j])
                swap(a[i],a[j]);
        }
    }
    for(int i=0;i<n;i++)
        cout<<a[i]<<" ";
    return 0;
}
